# AutoResearchLogMaker 环境配置模板

# ==== OpenAI / LLM ====
OPENAI_API_KEY=
OPENAI_BASE_URL=https://api.openai.com/v1
DEFAULT_LLM_MODEL=gpt-3.5-turbo

# ==== Database ====
DATABASE_URL=sqlite:///./lib/server/database.db

# ==== API Server ====
API_HOST=0.0.0.0
API_PORT=8000
API_RELOAD=true
API_LOG_LEVEL=info
API_WORKERS=1

# ==== Upload / Files ====
MAX_FILE_SIZE=52428800

# ==== OCR ====
OCR_LANGUAGE=ch
OCR_USE_GPU=false

# ==== Logging ====
LOG_LEVEL=INFO
LOG_FILE=
